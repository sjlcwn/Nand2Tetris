/**
 * 16-bit incrementer:
 * out = -in (arithmetic addition)
 */

CHIP Neg16 {
    IN in[16];
    OUT out[16];

    PARTS:
    // Put you code here:
    Not16(in=in,out=nin);
    Inc16(in=nin,out=out);
}